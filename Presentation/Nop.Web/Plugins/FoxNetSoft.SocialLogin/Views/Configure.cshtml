            toggleRequireGoogle();
            toggleRequireYahoo();
            toggleRequireTwitter();
            toggleRequireLinkedIn();
            toggleRequireMailRu();
            toggleRequireYandex();
            toggleRequireVk();
            toggleRequireAmazon();
            toggleRequireMicrosoft();
