        function toggleshowDebugInfo() {
            if ($('#@Html.IdFor(model => model.showDebugInfo)').is(':checked')) {
                $('#pnlclearlogfile, #pnlgetlogfile').show();
            }
            else {
                $('#pnlclearlogfile, #pnlgetlogfile').hide();
            }
        }

        function toggleRequireFacebook() {
            if ($('#@Html.IdFor(model => model.Facebook_Enable)').is(':checked')) {
                $('#pnlFacebook_ClientKeyIdentifier, #pnlFacebook_ClientSecret, #pnlFacebook_CallbackURI, #pnlFacebook_UsePopUpForm, #pnlFacebook_Birthday, #pnlFacebook_Avatar').show();
            }
            else {
                $('#pnlFacebook_ClientKeyIdentifier, #pnlFacebook_ClientSecret, #pnlFacebook_CallbackURI, #pnlFacebook_UsePopUpForm, #pnlFacebook_Birthday, #pnlFacebook_Avatar').hide();
            }
            $('#pnlFacebook_UsePopUpForm').hide();
        }


        function toggleRequireGoogle() {
